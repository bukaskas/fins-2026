generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Booking {
  id         String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name       String
  date       DateTime
  email      String
  phone      String
  service    String
  createdAt  DateTime @default(now()) @db.Timestamp(6)
  instructor String?
  time       String?
}
